{
    "rewrites": [
      {
        "source": "/countries/main.([a-z0-9]+).js",
        "destination": "main.js"
      },
      {
        "source": "/css/main.css",
        "destination": "main.css"
      },
      {
        "source": "countries/main.css",
        "destination": "main.css"
      },
      {
        "source": "/(.*)",
        "destination": "index.html"
      }
    ],
    "outputDirectory":"./build"
  }